{
  "name": "birdpaper-icon",
  "version": "0.0.11",
  "description": "基于 Remix icon 生成的 Vue3 图标组件库",
  "scripts": {
    "generate": "pnpm run -C packages generate",
    "scripts:dev": "pnpm run -C scripts dev",
    "scripts:build": "pnpm run -C scripts build",
    "build:components": "pnpm run -C packages build:components",
    "build:style": "pnpm run -C packages build:style",
    "build": "npm run build:components && npm run build:style"
  },
  "keywords": [
    "Remix icon",
    "icon-components",
    "svg-components",
    "Vue3"
  ],
  "author": "木亦Sam",
  "license": "MIT",
  "main": "es/index.js",
  "module": "es/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "sideEffects": [
    "dist/*",
    "es/**/style/*",
    "lib/**/style/*",
    "*.less"
  ],
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "homepage": "https://www.birdpaper.design/guide/icon",
  "repository": {
    "type": "git",
    "url": "https://github.com/birdpaper-team/birdpaper-icon"
  }
}
